
{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "target": "es2020",
    "module": "es2020",
    "outDir": "dist",
    "sourceMap": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}